    if (![sender isOn]) {
        companyRegViewHeight.constant = 55;
        companyRegView.clipsToBounds = YES;
        [companyRegView updateConstraintsIfNeeded];
    }
    else {
        companyRegViewHeight.constant = 230;
        companyRegView.clipsToBounds = YES;
        [companyRegView updateConstraintsIfNeeded];
    }
    
    
}
