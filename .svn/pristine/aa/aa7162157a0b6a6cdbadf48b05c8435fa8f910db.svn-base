#pragma mark - UITableViewDataSource

-(NSInteger)tableView:(UITableView *)tableView numberOfRowsInSection:(NSInteger)section
{
    return self.associationList.count;
}

-(UITableViewCell*)tableView:(UITableView *)tableView cellForRowAtIndexPath:(NSIndexPath *)indexPath
{
    MSAAssociationTableViewCell *cell = [tableView dequeueReusableCellWithIdentifier:@"associationCell"];
    if(cell==nil) {
        cell = [[MSAAssociationTableViewCell alloc] initWithStyle:UITableViewCellStyleDefault reuseIdentifier:@"associationCell"];
    }
    
    cell.selectionStyle = UITableViewCellSelectionStyleNone;
    cell.textLabel.textColor = [UIColor whiteColor];
    cell.fromLabel.text = [self.associationList objectAtIndex:indexPath.row];
    cell.typeLabel.text = [self.associationList objectAtIndex:indexPath.row];
    cell.radioImage.image = [UIImage imageNamed:@"radio.png"];
    
    return cell;
}

